{
  "name": "cms-for-personal-websites",
  "version": "1.0.0",
  "description": "Content Management System for YOUR personal website and/or portfolio!",
  "main": "build/app.js",
  "scripts": {
    "watch-ts": "tsc -w",
    "watch-sass": "sass --watch styles/index.scss assets/styles/bundle.css",
    "build": "tsc && sass --update styles/index.scss assets/styles/bundle.css && webpack",
    "prepare-deploy": "npm run build && node ./deploy/prepare.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasperLichte/CMS-for-personal-websites.git"
  },
  "keywords": [
    "cms",
    "portfolio"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JasperLichte/CMS-for-personal-websites/issues"
  },
  "homepage": "https://github.com/JasperLichte/CMS-for-personal-websitese#readme",
  "devDependencies": {
    "@types/node": "^11.9.4",
    "node": "^12.4.0",
    "sass": "^1.17.0",
    "typescript": "^3.3.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "fs-extra": "^8.0.1"
  }
}
